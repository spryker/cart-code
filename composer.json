{
  "name": "spryker/cart-code",
  "type": "library",
  "description": "CartCode module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/calculation": "^4.0.0",
    "spryker/cart-code-extension": "^1.1.0",
    "spryker/kernel": "^3.30.0",
    "spryker/quote": "^2.8.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/discount": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
